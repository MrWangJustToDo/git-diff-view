{
  "compilerOptions": {
    "rootDir": "src",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "stripInternal": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
